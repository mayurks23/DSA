   "source": [
    "class Solution:\n",
    "    def runningSum(self, nums: list[int]) -> list[int]:\n",
    "        for x in range(1,len(nums)):\n",
    "            nums[x] = nums[x-1] + nums[x]\n",
    "        return nums"
   ]
  },
